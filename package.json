    "@types/react-navigation": "2.13.7",
    "@types/react-redux": "6.0.0",
    "@types/react-test-renderer": "16.8.2",
    "babel-jest": "24.6.0",
    "detox": "12.1.3",
    "eslint": "5.15.3",
    "form-data": "2.3.3",
    "identicon.js": "2.3.3",
